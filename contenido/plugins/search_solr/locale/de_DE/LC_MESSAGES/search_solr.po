msgid "CLIENT_OPTIONS"
msgstr "Client Optionen"

msgid "OPTION"
msgstr "Option"

msgid "VALUE"
msgstr "Wert"

msgid "DESCRIPTION"
msgstr "Beschreibung"

msgid "DESCR_HOSTNAME"
msgstr "Der Name des Solr-Servers."

msgid "DESCR_PORT"
msgstr "Die Port-Nummer."

msgid "DESCR_PATH"
msgstr "Der Pfad zum Solr-Core."

msgid "DESCR_LOGIN"
msgstr "Der Benutzername für die HTTP-Autentifizierung, falls vorhanden."

msgid "DESCR_PASSWORD"
msgstr "Das Paßwort für die HTTP-Autentifizierung."

msgid "DESCR_SECURE"
msgstr "Boolean value indicating whether or not to connect in secure mode."

msgid "DESCR_TIMEOUT"
msgstr "This is maximum time in seconds allowed for the http data transfer operation. Default is 30 seconds."

msgid "DESCR_WT"
msgstr "The name of the response writer e.g. xml, phpnative."

msgid "DESCR_PROXY_HOST"
msgstr "The hostname for the proxy server, if any."

msgid "DESCR_PROXY_PORT"
msgstr "The proxy port."

msgid "DESCR_PROXY_LOGIN"
msgstr "The proxy username."

msgid "DESCR_PROXY_PASSWORD"
msgstr "The proxy password."

msgid "DESCR_SSL_CERT"
msgstr "File name to a PEM-formatted file containing the private key + private certificate (concatenated in that order). Please note the if the ssl_cert file only contains the private certificate, you have to specify a separate ssl_key file."

msgid "DESCR_SSL_KEY"
msgstr "File name to a PEM-formatted private key file only."

msgid "DESCR_SSL_KEYPASSWORD"
msgstr "Password for private key. The ssl_keypassword option is required if the ssl_cert or ssl_key options are set."

msgid "DESCR_SSL_CAINFO"
msgstr "Name of file holding one or more CA certificates to verify peer with."

msgid "DESCR_SSL_CAPATH"
msgstr "Name of directory holding multiple CA certificates to verify peer with."

msgid "DESCR_RELOAD"
msgstr "Reload the core according to its configuration file schema.xml."

msgid "DESCR_REINDEX"
msgstr "Reindex articles of current client. Articles that are offline, not searchable or hidden by protected categories will be skipped."

