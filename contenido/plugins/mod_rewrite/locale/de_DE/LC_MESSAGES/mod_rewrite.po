#: contenido/plugins/mod_rewrite/classes/controller/class.modrewrite_content_controller.php:218
#, php-format
msgid "Invalid separator for article, allowed is one of following characters: %s"
msgstr "Trenner f&uuml;r Kategorie ist ung&uuml;ltig, erlaubt ist eines der Zeichen: %s"

#: contenido/plugins/mod_rewrite/classes/controller/class.modrewrite_content_controller.php:225
#, php-format
msgid "Please specify separator (%s) for article words"
